.evaluation_more{height:40px;line-height: 40px;text-align: right;font-size: 1.4rem;width:100%;float: left;}
.evaluation_more a{display: block;color:#666;padding:0 10px;}
.evaluation_more i{float: right;}

.product_parameters,.product_features{padding:0px 10px 10px;border-bottom: 10px #f5f5f5 solid;float: left;width:100%;box-sizing: border-box;}
.product_parameters h2,.product_features h2{line-height: 60px;background: url(../images/bg05.png) left center no-repeat;background-size:4px auto;padding-left: 15px;font-size: 1.6rem;}
.product_parameters table{width:100%;border-collapse: collapse;}
.product_parameters table td{text-align: center;border:1px #eee solid;padding:10px;font-size: 1.4rem;line-height: 20px;}
.product_parameters .text_left{text-align: left;}

.teamwork_footer{position: fixed;bottom: 0;left: 0;height:50px;background: #fff;width: 100%;}
.teamwork_footer a{height:40px;padding:5px 0;line-height: 20px;}
.teamwork_footer .fh_index span{display: block;font-size: 1.4rem;color:#666;}
.teamwork_footer .fh_index{width:30%;float: left;text-align: center;}
.teamwork_footer .fh_index i{font-size: 2rem;display: block;}
.teamwork_footer .teamwork_btn{float: right;width:70%;text-align: center;background: #ea4b32;color:#fff;}
.teamwork_footer .teamwork_btn span{display: block;font-size: 1.6rem;}


/*物流跟踪*/
.logistics_num{height:40px;line-height: 40px;font-size: 1.4rem;padding:0 10px;}
.logistics_num span{color:#666;}
.logistics_num em{float: right;}
.logistics_title{font-size: 1.6rem;background: #fff;height:50px;line-height: 50px;padding:0 10px;border-bottom: 1px #eee solid;}
.logistics ul,.orders_info ul{background: #fff;padding:10px;}
.logistics ul li,.orders_info ul li{font-size: 1.4rem;position: relative;background: url(../images/bg07.png) 5px top no-repeat;}
.logistics ul li:last-child,.orders_info ul li:last-child{background: url(../images/bg08.png) 5px top no-repeat;}
.logistics ul li:first-child,.orders_info ul li:first-child{color:#fe0000;background: url(../images/bg06.png) left 15px no-repeat;}
.logistics ul li .logistics_track,.orders_info ul li .logistics_track{margin-left:30px;border-bottom: 1px #eee solid;padding:10px 0;}
.logistics ul li:last-child .logistics_track,.orders_info ul li:last-child .logistics_track{border-bottom: none;}



/*发表评价*/
.evaluation ul{padding: 10px;float: left;border-bottom: 1px #eee solid;}
.evaluation ul li:first-child{width:15%;margin-right: 5%;}
.evaluation ul li img{border:1px #eee solid;}
.evaluation ul li{width:80%;float: left;font-size: 1.4rem;overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2;}

.evaluation .textarea{padding:0 10px;}
.evaluation .textarea textarea{width:100%;padding:10px 0;height:92px;resize: none;}

.evaluation_file{border-bottom: 10px #f5f5f5 solid;}
.evaluation_file p{padding: 0 10px; width: 100%;box-sizing: border-box;float: left;position: relative;}
.evaluation_file .file input{width: 22%;margin-right: 2%;margin: 10px 0;font-size: 0;position: absolute;left: 10px;top: 0px;height: 22vw;opacity: 0;}
.evaluation_file .file img {float: left;width: 23%;margin: 10px 2% 10px 0;}

.store_rating{padding:0 10px;}
.store_rating > p{line-height: 40px;font-size: 1.4rem;}
.store_rating i{float:left;margin-right: 10px;font-size: 1.8rem;}

.store_rating .block{ width:100%;padding:10px 0;}
.store_rating .block .star_score{ float:left;}
.store_rating .block span{float: left;font-size: 1.4rem;margin-right: 10px;}
.store_rating .star_score { background:url(../images/stark2.png) no-repeat; width:160px; height:21px;  position:relative; }
.store_rating .star_score a{ height:21px; display:block; text-indent:-999em; position:absolute;left:0;}
#starttwo .star_score,#starttwo2 .star_score,#starttwo3 .star_score { background:url(../images/starky.png);}
#starttwo .star_score a:hover,#starttwo2 .star_score a:hover,#starttwo3 .star_score a:hover{ background:url(../images/starsy.png);left:0;}
#starttwo .star_score a.clibg,#starttwo2 .star_score a.clibg,#starttwo3 .star_score a.clibg{ background:url(../images/starsy.png);left:0;}




/*评论列表*/
.evaluation_uesr{height:30px;line-height: 30px;margin-bottom: 5px;}
.evaluation_uesr .uesr_img{float: left;}
.evaluation_uesr .uesr_img img{width:30px;height:30px;border-radius: 50%;}
.evaluation_uesr .uesr_name{margin-left: 10px;float: left;font-size: 1.4rem;}
.evaluation_uesr .uesr_time{float: right;color:#999;}

.evaluation_product{padding:20px 0px 0;display: -webkit-box;display: flex;width:100%;float: left;}
.evaluation_product_img{float: left;margin-right: 10px;}
.evaluation_product_img img{width:80px;height:88px;}
.evaluation_product_text{ -webkit-box-flex:1;flex:1;font-size: 1.4rem;padding-left: 10px;background: #f5f5f5;}
.evaluation_product_text h2{padding-top:10px;font-size: 1.4rem;height:48px;overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2;}
.evaluation_product_text p{color: #fe0000;}


/*图片点击放大*/
.evaluation_img .thumbnail {width:23%;float:left;margin:2% 2% 0 0;}
.evaluation_img .thumbnail img {width:100%;height:22vw;}
.baguetteBox-button {background-color: transparent !important;}

#baguetteBox-overlay{display:none;opacity:0;position:fixed;overflow:hidden;top:0;left:0;width:100%;height:100%;z-index:1000000;background-color:#222;background-color:rgba(0,0,0,.8);-webkit-transition:opacity .5s ease;transition:opacity .5s ease}
#baguetteBox-overlay.visible{opacity:1}
#baguetteBox-overlay .full-image{display:inline-block;position:relative;width:100%;height:100%;text-align:center}
#baguetteBox-overlay .full-image figure{display:inline;margin:0;height:100%}
#baguetteBox-overlay .full-image img{display:inline-block;width:auto;height:auto;max-height:100%;max-width:100%;vertical-align:middle;-moz-box-shadow:0 0 8px rgba(0,0,0,.6);box-shadow:0 0 8px rgba(0,0,0,.6)}
#baguetteBox-overlay .full-image figcaption{display:block;position:absolute;bottom:0;width:100%;text-align:center;line-height:1.8;white-space:normal;color:#ccc;background-color:#000;background-color:rgba(0,0,0,.6);font-family:sans-serif}
#baguetteBox-overlay .full-image:before{content:"";display:inline-block;height:50%;width:1px;margin-right:-1px}
#baguetteBox-slider{position:absolute;left:0;top:0;height:100%;width:100%;white-space:nowrap;-webkit-transition:left .4s ease,-webkit-transform .4s ease;transition:left .4s ease,-webkit-transform .4s ease;transition:left .4s ease,transform .4s ease;transition:left .4s ease,transform .4s ease,-webkit-transform .4s ease,-moz-transform .4s ease}
#baguetteBox-slider.bounce-from-right{-webkit-animation:bounceFromRight .4s ease-out;animation:bounceFromRight .4s ease-out}
#baguetteBox-slider.bounce-from-left{-webkit-animation:bounceFromLeft .4s ease-out;animation:bounceFromLeft .4s ease-out}@-webkit-keyframes bounceFromRight{0%,100%{margin-left:0}50%{margin-left:-30px}}@keyframes bounceFromRight{0%,100%{margin-left:0}50%{margin-left:-30px}}@-webkit-keyframes bounceFromLeft{0%,100%{margin-left:0}50%{margin-left:30px}}@keyframes bounceFromLeft{0%,100%{margin-left:0}50%{margin-left:30px}}
.baguetteBox-button#next-button,.baguetteBox-button#previous-button{top:50%;top:calc(50% - 30px);width:44px;height:60px}
.baguetteBox-button{position:absolute;cursor:pointer;outline:0;padding:0;margin:0;border:0;-moz-border-radius:15%;border-radius:15%;background-color:#323232;background-color:rgba(50,50,50,.5);color:#ddd;font:1.6em sans-serif;-webkit-transition:background-color .4s ease;transition:background-color .4s ease}
.baguetteBox-button:focus,.baguetteBox-button:hover{background-color:rgba(50,50,50,.9)}
.baguetteBox-button#next-button{right:2%}
.baguetteBox-button#previous-button{left:2%}
.baguetteBox-button#close-button{top:20px;right:2%;right:calc(2% + 6px);width:30px;height:30px}
.baguetteBox-button svg{position:absolute;left:0;top:0}


/*拼团流程*/
.process {padding:10px;}
.process p{font-size: 1.6rem;line-height: 30px;}
.process span{background: -webkit-linear-gradient(left, #fb9b8b , #f24e4d);height:30px;width:30px;text-align: center;border-radius: 50%;color:#fff;float:left;font-size: 2rem;margin-right: 10px;}
.process em{width:calc(100% - 40px);float: left;}
.process p:nth-child(2n){margin-left: 15px;border-left: 1px #f34d4d solid;padding: 0px 0 20px 15px;}
.process p:nth-child(2n) img{width:100%;padding-top: 15px;}


/*登录*/
.login_pwd{width:100%;float: left;}
.login_pwd a{float: right;padding:10px 10px 0;line-height: 40px;font-size: 1.2rem;}
.coo_login{text-align: center;width:100%;margin-top:30px;}
.coo_login h3{line-height: 40px;font-size: 1.2rem;background: url(../images/xian.png) center center no-repeat;margin:0 40px;}
.coo_login a{padding:0 10px;width:60px;display:inline-block;margin:auto;}
.coo_login img{width:60px;height:60px;}
.login .modify_phone_enter p input{width:calc(100% - 26px);}
.login .login_wj{line-height: 30px;font-size: 1.2rem;padding:10px 10px 0;display:inline-block;-webkit-box-sizing:border-box;box-sizing: border-box;color:#19a4e2;text-decoration:underline;}

.wx_avatar p{padding:0 10px;}
.wx_avatar p{font-size: 1.2rem;}
.wx_avatar p:nth-child(1){margin:20px auto;}
.wx_avatar p:nth-child(1) span{display: block;text-align: center;font-size: 1.4rem;margin-top:10px;}
.wx_avatar p:nth-child(1) img{width:90px;height:90px;border-radius: 50%;}


/*注册*/
.registered_enter{margin-top: 10px;}
.registered_enter p{background-color: #fff;width:100%;border-bottom: 1px #e7e7e7 solid;padding:0 10px;-webkit-box-sizing:border-box;box-sizing: border-box;height:50px;}
.registered_enter i{height:50px;line-height: 50px;float: left;}
.registered_enter input{height:20px;line-height:20px;padding:15px 0 15px 10px;font-size: 1.4rem;}
.registered_enter p input{width:calc(100% - 26px);}
.registered_enter p:last-child{border-bottom: none;}
.registered_enter p:nth-child(2) input{width:calc(100% - 106px);}
.registered_enter p:nth-child(3) input{width:calc(70% - 26px);}
.registered_enter p:nth-child(3) button{float: right;width:30%;border-left: 1px #e7e7e7 solid;height:50px;line-height: 50px;color:#de4943;}
.registered_enter .code{padding:5px 0;float:right;}
.registered_enter .code img{height:40px;width:80px;}
.registered_btn{margin-top: 20px;padding:0 10px;}
.registered_btn input{color:#fff;background-color:#de4943;display: block;border-radius: 5px;text-align: center;font-size: 1.6rem;height:40px;line-height: 40px;width:100%;}
.registered .reg_pro{color:#999;padding:0 10px;line-height: 20px;padding:15px 10px;font-size: 1.4rem;}
.registered .reg_pro a{color:#19a4e2;}


/*性别选择弹出框*/
.modal-open {overflow: hidden; }
.modal {display: none; overflow: hidden; position: fixed;top:0;right: 0; bottom: 0; left: 0; z-index: 1050; -webkit-overflow-scrolling: touch; outline: 0; }
.modal.fade .personal_gender,.gender.fade .personal_gender,.modal.fade .sign_out ,.modal.fade .num_input,.modal.fade .video,.modal.fade .success{-webkit-transform: translate(0, 100%); -ms-transform: translate(0, 100%); -o-transform: translate(0, 100%); transform: translate(0, 100%); -webkit-transition: -webkit-transform 0.3s ease-out; -moz-transition: -moz-transform 0.3s ease-out; -o-transition: -o-transform 0.3s ease-out; transition: transform 0.3s ease-out; }
.modal.in .personal_gender,.gender.in .personal_gender,.modal.in .sign_out,.modal.in .num_input,.modal.in .video,.modal.in .success{-webkit-transform: translate(0, 0); -ms-transform: translate(0, 0); -o-transform: translate(0, 0); transform: translate(0, 0); }
.modal-open .modal,.modal-open .gender {overflow-x: hidden;overflow-y: auto;}
.modal-dialog {position: relative; width: auto;}
.modal-backdrop {position: fixed; top: 0; right: 0; bottom: 0; left: 0; background-color: #000;height:100%;overflow:hidden;}
.modal-backdrop.fade {opacity: 0; filter: alpha(opacity=0); }
.modal-backdrop.in {opacity: 0.6; filter: alpha(opacity=60);}

.gender {display: none; overflow: hidden; position: fixed;right: 0; bottom: 0; left: 0; z-index: 1050; -webkit-overflow-scrolling: touch; outline: 0; }
.personal_gender p{line-height: 40px;height:40px;border-bottom: 1px #eee solid;text-align: center;background-color: #fff;}
.personal_gender p.cancel{border-bottom: none;margin-top: 10px;}
.personal_gender p button{width:100%;height:40px;line-height: 40px;}


/*分享*/
.personal_gender ul{background: #fff;border-bottom: 1px #eee solid; }
.personal_gender ul li{width:21%;display: inline-block;text-align: center;margin:15px 2% 0;}
.personal_gender ul li a{display: block;}
.personal_gender ul li span{display: block;font-size: 1.4rem;height:40px;line-height: 40px;}
.personal_gender ul li img{width:70%;max-width: 60px;}


/*选择地址弹窗*/
