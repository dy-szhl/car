@charset "utf-8";

html{font-size:62.5%;height:100%;}
body,div,dl,dt,dd,ul,ol,li,p,span,a,img,form,input,h1,h2,h3,h4,h5,h6 {margin: 0;padding: 0;}
body {width: 100%;min-width: 320px;font-size:12px;color:#333;background: #fff;font-family:Helvetica,Arial,"Microsoft YaHei","微软雅黑",sans-serif;line-height: 24px;word-wrap:break-word;word-break:break-all; -webkit-tap-highlight-color:rgba(0,0,0,0);}
ul li,ol li {list-style: none;}

h1,h2,h3,h4,h5,h6 {font-weight: normal;}
a {text-decoration: none;color:#333;}
img{border: none;display:block; margin:auto;max-width: 100%;}
a:focus{outline:none;}
em,strong{font-style: normal;}
textarea,input{word-wrap:break-word;word-break:break-all;padding:0px;color:#333;}
input, button, textarea, select {border:none;outline: none;background: none;font-family:Helvetica,Arial,"Microsoft YaHei","微软雅黑",sans-serif;-webkit-appearance: none;color:#333;-moz-appearance: none;appearance: none;}

.main{width:100%;float: left;}
.fl{float: left;width:100%;}
.fr{float: right;}
.wrap {padding:0 3%;}
.wrap1 {padding:0 0%;}
.clearfix:after {content: '.';display: block;clear: both;height: 0;visibility: hidden;}
.clearfix {*zoom:1;}

.mgtop{margin-top:50px;}
.wrapper{padding-bottom: 60px;}

#container{float: left;width:100%;}
#container .loading span{height:50px;display:inline-block;background:url(../images/loading2.gif) left center no-repeat;background-size:20px;padding-left:25px;}
#container .loading em{background:url(../images/xiala.png) left center no-repeat;background-size:20px;padding-left:25px;}


/*共用头部*/
.header{background:#fff;width: 100%;height: 50px;position: fixed;top: 0px;left: 0px;z-index:1001;text-align: center;line-height: 50px;border-bottom: 1px #eee solid;}
.header .header_return{display: block;height: 50px;line-height: 50px;position: absolute; left: 10px;top:0;}
.header .header_return i{font-size: 2.5rem;color:#666;}
.header .header_return img{width:30px;height:30px;margin:10px 0;}
.header .header_title{ width: 100%;height: 50px;font-size: 1.8rem;}
.header .header_title a{padding:0 10px;display: inline-block;height:48px;line-height: 48px;}
.header .header_title a.on{border-bottom: 2px #ff0000 solid;}
.header .header_carry{display: block;height: 50px;line-height: 50px;position: absolute; right: 3%;top:0;font-size: 1.6rem;}
.header .header_empty i{font-size: 1.8rem;}
.header .header_btn{position: absolute;right:10px;top: 0;height:50px;line-height: 50px;font-size: 1.6rem;}
.header .header_empty .icon-fenxiang{color:#666;}
.header .header_empty i.icon-fenxiang1{font-size: 2.5rem;}
.header .header_empty .picTab{width:24px;height: 24px;display: block;background: url(../images/bg37.png) center center no-repeat;background-size: 24px;margin-top: 12px;}
.header .header_empty .picTab.on{background: url(../images/bg36.png) center center no-repeat;background-size: 24px;}

.header_search{width:100%;height:50px;text-align: left;float: left;}
.header_search a,.header_search p{color: #999;border:1px #ccc solid;display: block;height:32px;line-height: 32px;width:70%;margin:9px auto;border-radius: 5px;padding-left:10px;box-sizing: border-box;font-size: 1.4rem;}
.header_search i{float: left;padding-right: 10px;}
.header_search input{width:calc(100% - 30px);height:20px;line-height: 20px;padding:6px 0;font-size: 1.4rem;box-sizing:content-box;}

.header.index_header{background: #163672;border:none;}
.header.index_header .header_search a{background: #fff;width:75%;}
.header.index_header .header_return i{color:#fff;}
.header.index_header .header_empty {right:10px;}
.header.index_header .header_empty i{color:#fff;font-size: 2.1rem;}
.header.index_header .header_empty span{position: absolute;right:-5px;top:7px;background: #cc0000;border-radius: 50%;width:16px;height:16px;font-size: 0.9rem;line-height: 16px;color:#fff;text-align: center;}

#pagination{text-align: center;padding-right: 0;}
#pagination .swiper-pagination-bullet-active{background-color:#d90106;}

.download{position:fixed;z-index:1002;width:100%;background:#fff;line-height:30px;padding:10px 5%;box-sizing: border-box;-webkit-box-sizing: border-box;top:0;border-bottom: 1px #eee solid;}
.download a{color:#333;}
.download img{width:30px;height:30px;float:left;padding-right:10px;}
.download .open_download{float:right;background:#c60202;color:#fff;height:30px;line-height:30px;width:80px;text-align:center;border-radius:2px;font-size:0.9rem;}


/*搜索*/
.search{padding:0 10px;}
.search .search_title{font-size: 1.4rem;line-height: 40px;line-height: 40px;float: left;width:100%;}
.search .search_title i{float: left;font-size: 1.8rem;}
.search .search_title strong{padding-left: 10px;}
.search .search_title i.icon-shanchu{float:right;}
.search .search_content{width:100%;float: left;margin:10px 0;}
.search .search_content a{background: #eee;float: left;margin:0 15px 10px 0;padding:0 12px;border-radius: 12px;}
.search .search_recording ul li{padding-left: 30px;float: left;width:100%;-webkit-box-sizing: border-box;
    box-sizing: border-box;}
.search .search_recording ul li a{height:30px;line-height: 30px;display: block;border-bottom: 1px #ccc solid;}


/*首页分类*/
.index_sort{border-bottom: 1px #efefef solid;margin-top: 20px;}
.index_sort ul li{width:25%;float: left;margin-bottom: 10px;}
.index_sort ul li a{display: block;text-align: center;color:#666;}
.index_sort ul li a img{width:60%;}
.index_sort ul li a span{display: block;line-height: 40px;font-size: 1.4rem;}

/*首页公告*/
.index_bulletin{padding:10px;}
.index_bulletin > span{font-size: 1.5rem;font-weight: bold;color:#163672;width:70px;float: left;border-right: 1px #efefef solid;height:24px;line-height: 24px;}
.index_bulletin > span i{color:#163672;font-size: 2rem;float: left;padding-right: 5px;}
.index_bulletin .txtMarquee{width:calc(100% - 91px);overflow:hidden; position:relative;float: left;height:24px;padding:0 10px;}
.txtMarquee .infoList li{font-size: 1.4rem;height:24px;line-height: 24px;display: block;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}

/*首页零售介绍*/
.index_reward{background: #f6f6f6;padding:10px 0;text-align: center;}
.index_reward_title h2{display: inline-block;font-weight: bold;font-size: 1.8rem;color:#163672;}
.index_reward_title span{width: 40px;height:4px;border-top:2px #163672 solid;display: inline-block;margin:0 20px;}
.index_reward p{font-size: 1.4rem;color:#7e7e7e;}
.index_reward p a{color:#cc0000;}


/*首页自营*/
.index_self .index_self_title{padding:15px 10px;}
.index_self .index_self_title p,.index_proxy_content .proxy_title p{float: left;line-height: 20px;}
.index_self .index_self_title p strong,.index_proxy_content .proxy_title p strong{font-size:1.5rem;float: left;}
.index_self .index_self_title p span,.index_proxy_content .proxy_title p span{font-size: 1rem;display:inline-block;background: url(../images/bg38.png) right center no-repeat; padding:0 20px 0 10px;color:#fff;margin-left:5px;background-size: auto 20px;}
.index_self .index_self_title a{float: right;font-size: 1.2rem;color:#666;line-height: 20px;}

.index_self_content{padding:0 10px;}
.index_self .index_self_list > li{width:calc(50% - 3px);margin-right: 6px;float: left;margin-bottom: 7px;}
.index_self .index_self_list > li:nth-child(2n){margin-right: 0;}
.slider-products img{width:100%;height:50vw;}
.index_self .index_self_list > li a{display: block;}
.index_self .index_self_list > li > a img{width:100%;height:24vw;}

.exshop_title{background: #4f74b9;padding:0 5px;}
.exshop_title h2{font-size:1.4rem;font-weight: bold;color:#fff;display: flex;}
.exshop_title h2 span{font-size: 1rem;color:#abc3f0;display: block;height:24px;overflow:hidden;flex: 1;padding-left: 5px;}
.exshop_title p{background: #fff;border-radius: 10px;height:20px;line-height: 20px;padding:0 10px;display: inline-block;margin-bottom: 4px;}
.exshop_content{background: #edf3ff;height:calc(24vw - 50px);}
@media (min-width:375px) {
	.exshop_content .txtMarquee{padding:5% 0;}
}


/*首页代理*/
.index_proxy_content .proxy_title{padding:10px 0;}
.index_proxy_content{padding:10px;}
.index_proxy_content ul li{width:calc(50% - 3px);margin-right: 6px;float: left;}
.index_proxy_content ul li:nth-child(2n){margin-right: 0;}
.index_proxy_content ul li > a{display: block;position: relative;}
.index_proxy_content .proxy_img img{width:100%;height:34vw;}
.index_proxy_content .proxy_text{position:absolute;left: 0;top:0;width:100%;background: rgba(0,0,0,0.5);color:#fff;text-align:center;height:34vw;z-index: 10;padding-top:10%;box-sizing: border-box;}
.index_proxy_content .proxy_text h2{font-size: 1.4rem;font-weight: bold;line-height: 20px;background: url(../images/bg39.jpg) center bottom no-repeat;padding-bottom: 6px;margin-bottom: 6px;}
.index_proxy_content ul li > a .proxy_text p{padding:0 10px;overflow: hidden;text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2;line-height: 20px;font-size: 1.2rem;}

.index_proxy_content .proxy_content{position: relative;}
.index_proxy_content .proxy_content a{color:#fff;}
.index_proxy_content .proxy_content .proxy_text p span{padding:0 6px;}
.index_proxy_content .proxy_content .proxy_text p{padding:0 10px;line-height: 20px;}

@media (max-width:320px) {
	.index_proxy_content .proxy_text{padding-top: 5%;}
}


/*首页vip*/
.vip_info{background: #fff9df url(../images/vip_bg01.png) center 20px no-repeat;background-size:90% auto;margin:15px 25px 30px 15px;border-radius: 20px;padding:50px 15px 30px;box-shadow: 10px 0 0 #f9bf41;-webkit-box-shadow: 10px 0 0 #f9bf41;}
.vip_info_top{background: url(../images/vip_bg02.png) right center no-repeat;background-size: auto 68px;padding:10px 0;margin:10px 0;}
.vip_info_top h3{color:#777974;font-size: 1.6rem;font-weight: bold;}
.vip_info_top h2{font-size: 2rem;color:#e04437;font-weight: bold;}
.vip_info_content dt{font-size: 1.8rem;font-weight: bold;margin-bottom:20px;}
.vip_info_content dd{font-size: 1.4rem;color: #7e786a;position: relative;padding-left:24px;margin-bottom: 15px;}
.vip_info_content dd span{width:18px;height:18px;line-height:18px;background:#ffdfa0;border-radius: 50%;display: inline-block;text-align:center;position: absolute;left: 0;top:3px;}
.vip_info_bottom{background: #f7da8a;padding:15px;color:#c66709;font-size: 1.4rem;}


/*首页酒商入驻*/
.index_Settle ul{padding:10px;}
.index_Settle ul li{float: left;width:calc(50% - 25px);margin-right: 6px;border:1px #ccc solid;border-radius: 10px;padding:10px;margin-bottom: 6px;}
.index_Settle ul li:nth-child(2n){margin-right: 0;}
.index_Settle ul li a{display: block;padding-left: 50px;}
.index_Settle ul li h2{font-size: 1.4rem;font-weight: bold;overflow: hidden;text-overflow: ellipsis;white-space:nowrap;}
.index_Settle ul li p{font-size: 1.2rem;line-height: 20px;color:#6c6c6c;overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2; }
.index_Settle ul li:nth-child(1){border-color: #278ccd;}
.index_Settle ul li:nth-child(2){border-color: #b775d6;}
.index_Settle ul li:nth-child(3){border-color: #dab132;}
.index_Settle ul li:nth-child(4){border-color: #27bbcd;}
.index_Settle ul li:nth-child(5){border-color: #df7134;}
.index_Settle ul li:nth-child(6){border-color: #d30b5a;}
.index_Settle ul li:nth-child(1) a{background: url(../images/icon10.png) left center no-repeat;background-size:40px;}
.index_Settle ul li:nth-child(2) a{background: url(../images/icon09.png) left center no-repeat;background-size:40px;}
.index_Settle ul li:nth-child(3) a{background: url(../images/icon11.png) left center no-repeat;background-size:40px;}
.index_Settle ul li:nth-child(4) a{background: url(../images/icon12.png) left center no-repeat;background-size:40px;}
.index_Settle ul li:nth-child(5) a{background: url(../images/icon13.png) left center no-repeat;background-size:40px;}
.index_Settle ul li:nth-child(6) a{background: url(../images/icon14.png) left center no-repeat;background-size:40px;}
.index_Settle ul li:nth-child(1) h2{color: #278ccd;}
.index_Settle ul li:nth-child(2) h2{color: #b775d6;}
.index_Settle ul li:nth-child(3) h2{color: #dab132;}
.index_Settle ul li:nth-child(4) h2{color: #27bbcd;}
.index_Settle ul li:nth-child(5) h2{color: #df7134;}
.index_Settle ul li:nth-child(6) h2{color: #d30b5a;}


/*首页产品*/
.swiper-container3{margin: 0 auto;position: relative;overflow: hidden;z-index: 1;}
.swiper-container3 a{display:block;}
.index_products{background: #f5f5f5;}
.index_products .index_tab{background:#fff;border-top:10px #f5f5f5 solid;margin-bottom: 10px;}
.index_products .index_tab a{width:50%;float: left;font-size:1.6rem;font-weight: bold;text-align: center;height: 40px;line-height: 40px;color:#666;}
.index_products .index_tab a.active{color:#b40b0b;}

.index_products ul li{width:calc(50% - 3px);float: left;background: #fff;margin-right: 6px;margin-bottom: 6px;position: relative;}
.index_products ul li:nth-child(2n){margin-right: 0;}
.index_products ul li a{display: block;}
.index_products .index_pro_img img{width:100%;height:54vw;}
.index_products .index_pro_name h2{font-size: 1.4rem;overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2;line-height: 20px;height:40px;}
.index_products .index_pro_name{padding:5px;}
.index_products .index_pro_name p strong{font-size: 1.6rem;color:#ff0000;}
.index_products .index_pro_name p del{color:#999;padding-left: 5px;}

.index_products .loadtip{text-align: center;line-height: 30px;}
.index_pro_label {position: absolute;top:4px;left:6px;color:#fff;font-size: 1rem;z-index:10;}
.index_pro_label span{background:#fba551;border-radius:2px;margin-right: 4px;padding:0 4px;height:18px;line-height:20px;display:inline-block;}
.index_pro_label em{background: #77beff;border-radius: 4px;padding:0 4px;}


/*公告详情*/
.bulletin_det{padding:20px 15px;}
.bulletin_det_title{text-align: center;border-bottom: 1px #dcdcdc solid;}
.bulletin_det_title h2{font-size: 1.6rem;line-height: 20px;}
.bulletin_det_title p{height:40px;line-height: 40px;color:#999;}
.bulletin_det_content{margin:15px 0;font-size: 1.4rem;color:#666;line-height: 30px;}


/*会员分销*/
.distribution_one{position: relative;padding-top: 12px;margin:20px 15px;}
.distribution_one_content{ border:1px #f89626 solid;padding:10px;}
.distribution_one h2{text-align:center;font-size: 1.8rem;color:#f89626;position: absolute;top:0;background: #fff;padding:0 20px;left: 0;right: 0;margin:auto;width:200px;font-weight: bold;}
.distribution_one h3{font-size: 1.6rem;color:#666;font-weight: bold;line-height: 40px;margin-top: 10px;}
.distribution_one h3:before{content: "";width:10px;height:10px;background: #f89626;display:inline-block;margin-right: 10px;}
.distribution_one p{color:#666;font-size: 1.4rem;line-height: 30px;}

.distribution_two{padding:0px 15px 20px;}
.distribution_two p{width:26%;padding:9vw 0;margin-right:11%;text-align:center;float: left;background: url(../images/bg48.png) center center no-repeat;background-size: 100% auto;font-size: 16px;line-height: 20px;color:#fff;}
.distribution_two p:nth-child(3n){margin-right: 0;}

.distribution_three{margin-bottom: 30px;}
.distribution_three_title{margin-bottom:20px;}
.distribution_three_title h2{font-size: 2rem;display: inline-block;font-weight: bold;padding:0 20px;width:140px;line-height: 40px;}
.distribution_three_title h2 em{color:#2451a2;}
.distribution_three_title span{width:30%;height:1px;background: #333;width:calc(50% - 90px);margin-top:20px;}
.distribution_three table{width:calc(100% - 30px);margin:0 15px;box-sizing: border-box;-webkit-box-sizing: border-box;border-collapse:collapse;border:1px #dfdfdf solid;}
.distribution_three table td{border: 1px #dfdfdf solid;text-align:center;padding:10px;font-size: 1.3rem;}
.distribution_three table tr td:nth-child(1){font-size: 1.5rem;}
.distribution_three table td span{color:#c40000;}
.distribution_three table td em{color: #2451a2;}
.distribution_three table td strong{font-size:1.5rem;}
.distribution_three p{font-size: 1.3rem;color:#999;padding:0 15px;}

.distribution_four{background:#efeeec;}
.distribution_four h2,.distribution_five h2{border-bottom: 1px #ddd solid;text-align:center;padding-top: 20px;color:#163672;font-weight: bold;}
.distribution_four h2 span,.distribution_five h2 span{font-size: 1.8rem;line-height: 40px;padding-top: 20px;border-bottom: 2px #cc0000 solid;display: inline-block;padding:0 20px;}
.distribution_four_text{padding:20px 15px;color:#666;font-size: 1.4rem;line-height: 30px;}
.distribution_four_text p{margin-bottom:10px;}

.distribution_five ul{margin-top:30px;}
.distribution_five ul li{float: left;width:42%;margin:0 4%;text-align:center;margin-bottom:20px;font-size: 1.3rem;color:#454545;}
.distribution_five ul li img{width:60px;height:60px;margin-bottom:10px;}




/*产品列表*/
.products_title{padding:0 10px;border-bottom: 1px #eee solid;box-sizing: border-box;-webkit-box-sizing: border-box;}
.products_title ul li{float: left;width:20%;text-align: center;height:50px;line-height: 50px;font-size: 1.4rem;}
.products_title ul li a{display: block;}
.products_title ul li.on a,.products_title ul li.cur2 a,.products_title ul li.cur3 a{color:#ff2525;}

.products_title ul li.jiage{background: url(../images/no01.png) 90% center no-repeat;background-size:7px;}
.products_title ul li.jiage.on{background: url(../images/no03.png) 90% center no-repeat;background-size:7px;}
.products_title ul li.jiage.cur3{background: url(../images/no02.png) 90% center no-repeat;background-size:7px;}
.products_title ul li.xiaoliang{background: url(../images/no01.png) 90% center no-repeat;background-size:7px;}
.products_title ul li.xiaoliang.on{background: url(../images/no03.png) 90% center no-repeat;background-size:7px;}
.products_title ul li.xiaoliang.cur2{background: url(../images/no02.png) 90% center no-repeat;background-size:7px;}
.products_title ul li select{background: url(../images/bg24.png) 100% center no-repeat;background-size:10px auto;display: block;line-height: 50px;text-align:center;width:100%;height:50px;}
.products_title ul li.on select{color:#ff2525;background: url(../images/bg23.png) 100% center no-repeat;background-size:10px auto;}
.products_title ul li.on option{color:#333;}

.list li{float: left;padding:10px 10px 0;position: relative;width:100%;box-sizing: border-box;-webkit-box-sizing: border-box;height:120px;}
.list li a{width:100%;position: absolute;height:100%;left: 0;z-index: 10;top:0;}
.list li > img{float: left;width:110px;height:110px;}
.list .products_info{width:calc(100% - 120px);padding-left: 10px;float: left;border-bottom: 1px #eee solid;}
.list li:last-child .products_info{border:none;}

.products_info .proname{height:60px;}
.products_info .proname h2{font-size: 1.4rem;max-height:40px;line-height: 20px;overflow: hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.products_info .proname p{height:20px;line-height: 20px;}
.products_info .proname span{font-size: 1rem;color:#fff;border-radius:3px;padding:0 4px;height:18px;line-height:18px;display:inline-block;}
.products_info .proname span.zhangs{background: #ff5758;}
.products_info .proname span.maizeng{background: #87aef1;margin-left:10px;}
.products_info .price strong{font-size: 1.4rem;color:#ff2525;margin-right:5px;font-weight:normal;}
.products_info .price s{color:#666;}
.products_info .pro{height:20px;line-height: 20px;margin-bottom: 4px;}
.products_info .pro img{float: left;width:50px;margin-right: 5px;}
.products_info .pro span{color:#666;padding-left:5px;float:right;}

.list2{float: left;background: #f5f5f5;padding:2% 0 0;width:100%;}
.list2 li{width:49%;margin-right: 2%;float: left;background: #fff;margin-bottom: 2%;position: relative;}
.list2 li:nth-child(2n){margin-right: 0;}
.list2 li a{width:100%;position: absolute;height:100%;left: 0;z-index: 10;top:0;}
.list2 li > img{width:100%;}
.list2 li .products_info{padding:5px;}



/*筛选begin*/
.filter{width: 100%;height: 100%;position:fixed;top:0;right:0;display: none;z-index:1001;}
.filterMask{width: 100%;min-height: 100%;position:absolute;top:0;right:0;background-color: #000;opacity: 0.8;z-index:50;}
.filterFrame{width: 100%;min-height: 100%;position:absolute;right:0;top:0;z-index:90;box-sizing: border-box; -moz-box-sizing: border-box; -webkit-box-sizing: border-box;}
.filterFrame .filterList,.filterFrame .select{width: 80%;overflow-y:auto;background:#fff;position: absolute;top:0px;right: 0;height: 100%;}
.filterFrame .select{min-height:100%;}
.filterFrame > .close{width: 20%;min-height: 100%;position: absolute;top:0;left: 0;}

.filterList > .close{height:40px;line-height: 40px;border-bottom: 10px #f5f5f5 solid;text-align: center;}
.filterList > .close i{position: absolute;left:0px;font-size: 2rem;width:30px;}
.filterList > .close span{text-align: center;font-size: 1.6rem;display: block;}
.filterList .result{padding: 10px 15px 0;border-bottom:1px solid #c9c9c9;overflow: hidden;display: none;}
.result .condition{width: auto;height: 20px;display:block;float: left;margin-right: 20px;}
